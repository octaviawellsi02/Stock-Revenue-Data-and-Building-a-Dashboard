import pandas as pd
import matplotlib.pyplot as plt

# Assuming you have a DataFrame with GameStop stock data, let's call it 'gme_stock_data'
# gme_stock_data = pd.read_csv('gme_stock_data.csv')

# Plotting the data
plt.plot(gme_stock_data['Date'], gme_stock_data['Close'], label='GameStop Stock Price')

# Adding a title to the graph
plt.title('GameStop Stock Price Over Time')

# Adding labels to the axes
plt.xlabel('Date')
plt.ylabel('Stock Price')

# Displaying the legend
plt.legend()

# Show the plot
plt.show()



